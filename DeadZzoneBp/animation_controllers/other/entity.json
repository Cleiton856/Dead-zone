{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.npc_death": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"death": "q.health <= 1"
						}
					]
				},
				"death": {
					"on_entry": [
						"@s death",
						"/tag @s add death"
					]
				}
			}
		},
		"controller.animation.add_blood": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"blood": "q.life_time > 0.5"
						}
					],
					"on_entry": [
						"v.add_blood_delay = q.life_time;"
					]
				},
				"blood": {
					"transitions": [
						{
							"default": "(q.life_time - v.add_blood_delay) >= 5"
						}
					],
					"on_entry": [
						"v.add_blood_delay = q.life_time;",
						"/execute at @s run scoreboard players add @s[m=!c,tag=!bleeding] blood 1",
						"/execute at @s run scoreboard players set @s[scores={blood=3500..}] blood 3500"
					]
				}
			}
		},
		"controller.animation.removd_medical": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"medical": "q.life_time > 0.5"
						}
					],
					"on_entry": [
						"v.remove_medical_delay = q.life_time;"
					]
				},
				"medical": {
					"transitions": [
						{
							"default": "(q.life_time - v.remove_medical_delay) >= 20"
						}
					],
					"on_entry": [
						"v.remove_medical_delay = q.life_time;",
						"/execute at @s run scoreboard players remove @s medicalUsageCount 1",
						"/execute at @s run scoreboard players set @s[scores={medicalUsageCount=..0}] medicalUsageCount 0"
					]
				}
			}
		},
